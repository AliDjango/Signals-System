    "# 14.2\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "omega = 3\n",
    "A = 10\n",
    "x = lambda t: t\n",
    "y = lambda t: (A + x(t)) * np.cos(omega * t)\n",
    "\n",
    "\n",
    "t = np.linspace(0, 100 , 100)\n",
    "\n",
    "\n",
    "y_values = y(t)\n",
    "\n",
    "plt.plot(t, y_values)\n",
    "plt.show()"
   ]
